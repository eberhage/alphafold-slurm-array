{
  "af3_container_path": "/path/to/af3_container.sif",
  "af3_model_path": "/path/to/model",
  "af3_db_path": "/path/to/db",
  "datapipeline_partition": "cpupartition",
  "inference_partition": "gpupartition",
  "gpu_profiles": {
    "40g": {
      "gres": "a100-40g",
      "token_limit": 3072
    },
    "80g": {
      "gres": "a100-80g",
      "token_limit": 5120
    },
    "80g-XLA": {
      "gres": "a100-80g",
      "token_limit": 6000,
      "enable_xla": true
    }
  }
}
